{
  "name": "lektor",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@openfonts/roboto-slab_all": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "bootstrap": "~3.3.0",
    "chai": "^4.2.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.4",
    "event-source-polyfill": "^1.0.21",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.3.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.2",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "prettier": "prettier --write static/js static/less",
    "lint": "prettier --check static/js static/less && eslint --ext .js,.jsx static/js",
    "fix-lint": "eslint --ext .js,.jsx static/js --fix",
    "test": "nyc mocha",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov",
    "dev": "webpack --watch --config ./static/webpack.config.js --context ./static",
    "webpack": "webpack --config ./static/webpack.config.js --context ./static"
  },
  "targets": "defaults",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "mocha": {
    "spec": "static/js/**/*.test.js",
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "author": "",
  "license": "ISC"
}
